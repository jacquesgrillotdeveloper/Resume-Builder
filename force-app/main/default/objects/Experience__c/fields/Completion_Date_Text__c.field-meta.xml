<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Completion_Date_Text__c</fullName>
    <description>Formula field to display Date of Experience record completion in Month Year format.</description>
    <externalId>false</externalId>
    <formula>CASE(MONTH(Completion_Date__c),
	1, &quot;January&quot;,
	2, &quot;February&quot;,
	3, &quot;March&quot;, 
	4, &quot;April&quot;, 
	5, &quot;May&quot;, 
	6, &quot;June&quot;,
	7, &quot;July&quot;,
	8, &quot;August&quot;,
	9, &quot;September&quot;,
	10, &quot;October&quot;,
	11, &quot;November&quot;,
	12, &quot;December&quot;,
	&quot;&quot;)
&amp;&quot; &quot;&amp;TEXT(YEAR(Completion_Date__c))</formula>
    <label>Completion Date</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
